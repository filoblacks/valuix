{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.valuix.xyz" }],
      "destination": "https://valuix.xyz/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
